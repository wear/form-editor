jest.dontMock('../src/RowComponent');

var rowComponent = require('../src/RowComponent');

describe('row component', function() {
  it('render code', function() {
    var code = [{tag: 'code', body: 'code'}]
    var row = TestUtils.renderIntoDocument(
      React.createElement("rowComponent", {lines: code}));

    expect(React.findDOMNode(row.refs.body).textContent).toEqual('加载中...');
  })
});
