jest.dontMock('../RowComponent');

var React = require('react'),
    ReactDOM = require('react-dom');
    rowComponent = require('../RowComponent');
    TestUtils = require('react-addons-test-utils')


describe('row component', function() {
  it('render code', function() {
    var code = [{tag: 'code', body: 'code'}]
    var row = TestUtils.renderIntoDocument(React.createElement("rowComponent", {lines: code}));

    console.log(ReactDOM.findDOMNode(row))
    // expect(ReactDOM.findDOMNode(row.refs.body).textContent).toEqual('加载中...');
  })
});
