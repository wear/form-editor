jest.dontMock('../RowComponent');

var React = require('react'),
    ReactDOM = require('react-dom');
    RowComponent = require('../RowComponent');
    TestUtils = require('react-addons-test-utils')

describe('row component', function() {
  it('render code', function() {
    var codeLine = [{tag: 'code', body: 'code'}]
    var shallowRenderer = TestUtils.createRenderer();

    shallowRenderer.render(React.createElement(RowComponent, { lines: codeLine }))
    var component = shallowRenderer.getRenderOutput();

    expect(component.props.children).toEqual('MyComponent');
  })
});
